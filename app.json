{
  "expo": {
    "name": "FocaVestFrontRn",
    "slug": "FocaVestFrontRn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-iconFV.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-iconFV.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.leowbk.FocaVestFrontRn"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/FocaVestPLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8933d04f-0662-4293-bcb5-1ee94a19943d"
      }
    },
    "owner": "leowbk"
  }
}
